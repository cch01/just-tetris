{
  "name": "react-ts-vite-eslint-prettier-tailwind",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint src --report-unused-disable-directives",
    "types": "bash -c tsc --project ./tsconfig.json --noEmit",
